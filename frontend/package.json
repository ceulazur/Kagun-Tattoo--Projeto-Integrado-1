{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "main": "electron-main.js",
  "description": "Kagun Tattoo Application",
  "author": "Danyel",
  "scripts": {
    "dev": "vite",
    "dev:renderer": "vite --config vite.renderer.config.js",
    "build": "vite build",
    "start": "electron .",
    "build:electron": "electron-builder",
    "package": "electron-builder"
  },
  "dependencies": {
    "@ant-design/pro-components": "^2.8.2",
    "@ant-design/pro-form": "^2.31.2",
    "antd": "^5.22.1",
    "axios": "^1.7.7",
    "babel-plugin-react-router": "^0.0.1",
    "bootstrap": "^5.3.3",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "jwt-decode": "^4.0.0",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.8",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "react-input-mask": "^2.0.4",
    "react-router-dom": "^6.28.0",
    "vite-plugin-electron": "^0.8.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "electron": "^25.3.1",
    "electron-builder": "^22.11.7",
    "eslint": "^9.13.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "typescript": "^5.1.3",
    "vite": "^5.4.10"
  },
  "build": {
    "appId": "com.kagun.tattoo",
    "productName": "Kagun Tattoo",
    "files": [
      "dist/**/*",
      "dist-electron/**/*",
      "package.json",
      "electron-main.js",
      "preload.js"
    ],
    "extraResources": [
      {
        "from": "bin/${os}",
        "to": "bin",
        "filter": [
          "**/*"
        ]
      }
    ],
    "compression": "maximum",
    "extends": null,
    "directories": {
      "buildResources": "build"
    },
    "win": {
      "artifactName": "kagun-tattoo-setup.exe",
      "target": [
        "portable"
      ]
    }
  }
}
